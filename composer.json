{
  "name": "mauchede/rancher-api",
  "type": "library",
  "description": "Rancher API is a set of PHP classes for interacting with Rancher",
  "keywords": [
    "api",
    "client",
    "rancher"
  ],
  "homepage": "http://github.com/mauchede/rancher-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Morgan Auchede",
      "email": "morgan.auchede@gmail.com",
      "homepage": "http://github.com/mauchede"
    }
  ],
  "autoload": {
    "psr-4": {"Mauchede\\RancherApi\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Mauchede\\RancherApi\\Tests\\": "tests/"}
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.0",
    "jms/serializer": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
